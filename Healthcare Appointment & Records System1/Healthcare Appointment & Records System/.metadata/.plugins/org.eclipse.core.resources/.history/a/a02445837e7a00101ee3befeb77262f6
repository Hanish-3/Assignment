package com.example.patientservice.dto;

import java.time.LocalDate;

public record PatientRequestDTO(
    String firstName,
    String lastName,
    LocalDate dateOfBirth,
    String gender,
    String address,
    String phoneNumber,
    String email,
    String insuranceProvider,
    String insuranceNumber
) {}

public record PatientResponseDTO(
    Long id,
    String firstName,
    String lastName,
    String email,
    String phoneNumber
) {}
